{
   "action": {
      "default_icon": "static/icon/icon.png",
      "default_popup": "static/web/options.html"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "js": [ "jquery.min.js", "content.js"],
      "matches": [ "*://*/*" ]
   } ],
   "description": "beta",
   "externally_connectable": {
      "matches": [ "*://*.amazon.de/*" ]
   },
   "host_permissions": [ "*://*/*" ],
   "icons": {
      "128": "static/icon/icon.png",
      "16": "static/icon/icon.png",
      "48": "static/icon/icon.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Ersa",
   "options_page": "static/web/options.html",
   "permissions": [ "tabs", "activeTab", "windows", "storage", "notifications", "unlimitedStorage"],
   "short_name": "Y_Y",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.9.5",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "static/web/options.html", "jquery.min.js" ]
   } ]
}
